                "properties": {
                    "adapter": {
                        "enum": [
                            "faker"
                        ]
                    },
                    "count": {
                        "type": "number"
                    }
                }
            },
            "s_memory": {
                "properties": {
                    "adapter": {
                        "enum": [
                            "memory"
                        ]
                    }
                }
            },
            "s_mongodb": {
                "properties": {
                    "adapter": {
                        "enum": [
                            "mongodb"
                        ]
                    },
                    "url": {
                        "type": "string"
                    },
                    "database": {
                        "type": "string"
                    },
                    "id": {
                        "type": "string"
                    },
                    "collection": {
                        "type": "string",
                        "description": "collection name in nosql storages if different from $id"
                    },
                    "aggregate": {
                        "description": "filter documents pipeline criteria including $group, $sort, $match, and ... or SQL",
                        "type": ["array", "object"]
